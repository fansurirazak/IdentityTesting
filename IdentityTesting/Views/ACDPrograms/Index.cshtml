@model IEnumerable<IdentityTesting.Models.ACDProgram>

@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table" id="woopwoop">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.ACDProgramName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ACDProgramCode)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.ACDProgramName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ACDProgramCode)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.ACDProgramID">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.ACDProgramID">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.ACDProgramID">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
